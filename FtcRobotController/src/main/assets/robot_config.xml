<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot>
    <DeviceInterfaceModule name="Device Interface Module" serialNumber="AL00VDT3">
        <Gyro name="gyro_1" port="0"/>
        <Gyro name="gyro_2" port="1"/>
    </DeviceInterfaceModule>
    <MotorController name="Motor Controller Chain" serialNumber="AL00VTFM">
        <Motor name="motor_chain" port="1"/>
    </MotorController>
    <MotorController name="Motor Controller Drive Left" serialNumber="AL00UYTA">
        <Motor name="motor_drive_left" port="1"/>
        <Motor name="motor_lift_left" port="2"/>
    </MotorController>
    <MotorController name="Motor Controller Arm" serialNumber="AL00UWHE">
        <Motor name="motor_arm_lift" port="1"/>
        <Motor name="motor_arm_retract" port="2"/>
    </MotorController>
    <ServoController name="Servo Controller" serialNumber="AI02QTME">
        <Servo name="release_left" port="1"/>
        <Servo name="superblocker_vertical" port="2"/>
        <Servo name="superblocker_left" port="3"/>
        <Servo name="dumper" port="4"/>
        <Servo name="superblocker_right" port="5"/>
        <Servo name="release_right" port="6"/>
    </ServoController>
    <MotorController name="Motor Controller Drive Right" serialNumber="AH00QIKP">
        <Motor name="motor_drive_right" port="1"/>
        <Motor name="motor_lift_right" port="2"/>
    </MotorController>
</Robot>
